{
  "name": "DIVE IN",
  "description": "Extension for Tab hierarchy definition and marking",
  "version": "1.0",
  "icons": {
    "128": "icons/icon_128.png"},
  "manifest_version": 2,

  "permissions": [
    "tabs"
  ],

  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["contentScripts.js"]
}],

  "background": {
    "scripts" : [
      "background.js"
    ]
  },


  "sidebar_action": {
    "default_page": "sidebar/sidebar.html",

    "default_icon": {
      "16": "icons/icon_16.png"
    },

    "default_title": "DIVE IN",
    "use_navigation_bar": false
  }
}
